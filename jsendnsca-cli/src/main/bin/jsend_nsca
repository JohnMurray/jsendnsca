#!/bin/bash

export CLASSPATH=jsendnsca-cli-1.1.jar
export CLASSPATH=$CLASSPATH:../lib/jsendnsca-core-1.1.jar
export CLASSPATH=$CLASSPATH:../lib/commons-cli-1.1.jar
export CLASSPATH=$CLASSPATH:../lib/commons-lang-2.4.jar
export CLASSPATH=$CLASSPATH:../lib/commons-io-1.4.jar

java -classpath $CLASSPATH com.googlecode.jsendnsca.Main $@
